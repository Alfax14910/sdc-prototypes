---
title: Typeaheads
group: Typeaheads
version: 0.1.17
layout: typeaheads/views/_master
---
{% from "components/field/_macro.njk" import onsField %}
{% from "components/input/_macro.njk" import onsInput %}
{% from "components/button/_macro.njk" import onsButton %}

{% block content %}
    <form action="{{ pageInfo.parentPath }}/date-of-birth.html">
       {% call onsField({
           "id": "name",
           "legend": "<h1 class=\"u-fs-l\">What is your name?</h1>"
       }) %}
            {{
                onsInput({
                    "id": "first-name",
                    "name": "first-name",
                    "label": {
                        "text": "First or given name"
                    }
                })
            }}
            {{
                onsInput({
                    "id": "middle-name",
                    "name": "middle-name",
                    "label": {
                        "text": "Middle names"
                    }
                })
            }}
            {{
                onsInput({
                    "id": "last-name",
                    "name": "last-name",
                    "label": {
                        "text": "Surname or family name"
                    }
                })
            }}
        {% endcall %}
        {{
            onsButton({
                "text": "Save and continue",
                "classes": "u-mt-l"
            })
        }}
    </form>
    <script>
        // Wake up heroku instance
        var xhttp = new XMLHttpRequest();
        xhttp.open("GET", "https://ons-typeahead-prototypes.herokuapp.com");
        xhttp.send();
    </script>
{% endblock %}


