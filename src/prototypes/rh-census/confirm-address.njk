---
title: Confirm address
group: Census
version: 8.0.1
layout: rh-census/views/_master
---
{% from "components/question/_macro.njk" import onsQuestion %}
{% from "prototypes/rh-census/components/uac-entry/_macro.njk" import onsUACEntry %}
{% from "components/button/_macro.njk" import onsButton %}
{% from "components/panel/_macro.njk" import onsPanel %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "action": "/prototypes/rh-census/enter-mobile-number.html"
    }
} %}

{% block main %}
    <a class="previous-link js-previous" href="{{ pageInfo.parentPath }}">Previous</a>
    {% call onsQuestion({
        "title": "Is this address correct?",
        "useFieldset": true
    }) %}

    <p>
        94 Osborne Road<br/>
        Kingarth<br/>
        PA20 6PG
    </p>

    {% from "components/radios/_macro.njk" import onsRadios %}
    {{ 
        onsRadios({
            "name": "food",
            "radios": [
                {
                    "id": "yes",
                    "label": {
                        "text": "Yes, this address is correct"
                    },
                    "value": "yes"
                },
                {
                    "id": "no",
                    "label": {
                        "text": "No, I need to change it"
                    },
                    "value": "no"
                }
            ]
        }) 
    }}
    {% endcall %}

    {{
        onsButton({
            "text": "Continue",
            "classes": "u-mt-l"
        })
    }}

{% endblock %}

<scrip>

</scrip>
