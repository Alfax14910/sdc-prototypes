---
version: 20.0.0
layout: census-support/views/_master
---
{% from "components/question/_macro.njk" import onsQuestion %}
{% from "components/fieldset/_macro.njk" import onsFieldset %}
{% from "components/button/_macro.njk" import onsButton %}
{% from "components/radios/_macro.njk" import onsRadios %}
{% from "components/panel/_macro.njk" import onsPanel %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "action":  pageInfo.rootPath + "/code-text-sent.html"
    }
} %}
{% block preMain %}
    <div class="u-mt-s">
        <a href="{{ pageInfo.rootPath }}/what-is-your-mobile-phone-number.html">Previous</a>
    </div>
{% endblock %}
{% block main %}
    {% call onsQuestion({
        "title": "Is this mobile number correct?",
        "useFieldset": true
    }) %}
        <p>01234567890</p>
        {{
            onsRadios({
                "name": "mobile-correct",
                "radios": [
                    {
                        "id": "yes",
                        "label": {
                            "text": "Yes, send the text message"
                        },
                        "value": "yes"
                    },
                    {
                        "id": "no",
                        "label": {
                            "text": "No, I need to change it"
                        },
                        "value": "no"
                    }
                ]
            })
        }}
        {{ 
            onsButton({
                "text": "Continue",
                "classes": "u-mt-xl u-mb-xl"
            }) 
        }}
    {% endcall %}

{% endblock %}
