version: 20.0.0
layout: census-support/views/_master
---
{% from "components/question/_macro.njk" import onsQuestion %}
{% from "components/fieldset/_macro.njk" import onsFieldset %}
{% from "components/button/_macro.njk" import onsButton %}
{% from "census-support/components/input/_macro.njk" import onsInput %}
{% from "components/panel/_macro.njk" import onsPanel %}

{% block preMain %}
    <div class="u-mt-s">
        <a href="{{ pageInfo.rootPath }}/confirm-mobile-phone-number.html">Previous</a>
    </div>
{% endblock %}
{% block main %}
    {% call onsPanel({ 
        "type": "success",
        "icon": "check-green",
        "classes": "u-mb-l"
    }) %}
        <h1>We have sent a text to 01234567890</h1>
        <p>The text message with a new household access code should arrive soon for you to start your census</p>
    {% endcall %}

    <h2 class="u-fs-m u-mb-xs">Didn’t receive a text?</h2>
    <p class="u-mb-l">It can take a few minutes for the text to arrive. If it doesn’t arrive, you can <a class="js-link" href="">request a new access code</a>.</p>

{% endblock %}
