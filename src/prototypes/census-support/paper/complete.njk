---
version: 23.0.1
layout: census-support/views/_master
---
{% from "components/question/_macro.njk" import onsQuestion %}
{% from "components/fieldset/_macro.njk" import onsFieldset %}
{% from "components/button/_macro.njk" import onsButton %}
{% from "census-support/components/input/_macro.njk" import onsInput %}
{% from "components/panel/_macro.njk" import onsPanel %}

{% block preMain %}
    <div class="u-mt-s">
    </div>
{% endblock %}
{% block main %}
    {% call onsPanel({ 
        "type": "success",
        "icon": "check",
        "classes": "u-mb-no"
    }) %}
        <h1>You can now fill in your paper questionnaire</h1>
    {% endcall %}

    <form onsubmit="return false;">
        <div class="btn-group">
            {{
                onsButton({
                    "id": "done",
                    "text": "Done",
                    "classes": "u-mt-xl u-mb-no"
                })
            }}
        </div>
    </form>
    <script>
        var doneButton = document.getElementById('done');

        document.getElementById("done").addEventListener("click", function(e) {
            e.preventDefault();

            window.location.href = '{{ pageInfo.rootPath }}/index.html';

        });
    </script>
{% endblock %}
