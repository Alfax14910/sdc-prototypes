---
version: 14.4.4
layout: typeahead-nisra-schools/views/_master
---
{% from "components/button/_macro.njk" import onsButton %}
{% from "components/radios/_macro.njk" import onsRadios %}
{% from "components/question/_macro.njk" import onsQuestion %}
{% from "typeahead-nisra-schools/components/input/_macro.njk" import onsInput %}

{% block main %}
    <a class="previous-link js-previous" href="{{ pageInfo.rootPath }}/country-of-birth.html">Previous</a>
    <form action="{{ pageInfo.parentPath }}/travel.html?previous={{ pageInfo.path }}.html">
        {% call onsQuestion({
            "title": 'What address do you travel to for your course of study, including school?'
        }) %}
        {{
            onsInput({
                "id": "study-address",
                "classes": "input--w-auto",
                "label": {
                    "text": "Enter an address or postcode and select from results"
                },
                "typeahead":{
                    "instructions": "Use up and down keys to navigate suggestions once you\'ve typed more than two characters. Use the enter key to select a suggestion. Touch device users, explore by touch or with swipe gestures.",
                    "content": {
                        "aria_you_have_selected": "You have selected",
                        "aria_min_chars": "Enter 3 or more characters for results.",
                        "aria_one_result": "There is one result available.",
                        "aria_n_results": "There are {n} results available.",
                        "aria_limited_results": "Results have been limited to 10. Enter more characters to improve results.",
                        "more_results": "Continue entering to improve results",
                        "results_title": "Select an address"
                    },
                    "typeaheadData": "https://gist.githubusercontent.com/jrbarnes9/9136942c965eb8f76f194e90e497d264/raw/dde39f4054d53bdbf8a2eab30aa74db2d17b5e81/nisra-schools.json"
                }
            })
        }}
        {% endcall %}
        {{
           onsButton({
                "text": "Save and continue",
                "classes": "u-mt-l"
            })
        }}
    </form>
{% endblock %}
